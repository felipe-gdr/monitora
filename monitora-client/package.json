    "vinyl-source-stream": "^1.1.0"
  },
  "devDependencies": {
    "5to6-codemod": "^1.7.1",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-preset-react": "^6.24.1",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.17",
    "codecov.io": "^0.1.6",
    "commitizen": "^2.9.6",
    "css-loader": "^0.28.7",
    "cz-conventional-changelog": "^2.0.0",
    "enzyme": "^2.8.0",
    "eslint": "^4.8.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.4",
    "html-webpack-plugin": "^2.29.0",
    "jest": "^21.2.1",
